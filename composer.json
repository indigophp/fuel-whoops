{
    "name": "indigophp/fuel-whoops",
    "type": "fuel-package",
    "description": "Whoops! integration replacing the core error/exception handler",
    "license": "MIT",
    "keywords": ["exception", "error", "whoops"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "composer/installers": "~1.0",
        "filp/whoops": "1.2.*@dev"
    },
    "require-dev": {
        "codeception/codeception": "~2.0.0"
    },
    "autoload": {
        "psr-0": {
            "Whoops\\": "lib/"
        }
    },
    "extra": {
        "installer-name": "whoops"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
